{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "lib": ["es2017", "dom", "dom.iterable"],
    "noImplicitAny": false
  },
  "include": ["src/**/*"]
}